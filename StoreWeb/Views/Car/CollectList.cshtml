@{
    ViewData["Title"] = "CollectList";
}

<h2>Collect List</h2>
<ul>
	@for (int i = (int)ViewData["year"]; i < DateTime.Now.Year - 10; i++)
	{
		<li><b>@ViewData["brand"]</b>: year@i</li>
	}
</ul>
